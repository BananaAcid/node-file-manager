{
  "name": "node-file-manager-esm",
  "version": "2.1.1",
  "description": "File manager, based on Koa and Angular.js. Standalone, Koa-mountable. Import-able as ES6 and Babel module.",
  "main": "index.mjs",
  "engines": {
    "node": ">=10.5"
  },
  "scripts": {
    "start": "DEBUG=fm:* node --experimental-modules ./bin/node-file-manager-esm.mjs",
    "start-debug": "DEBUG=* node --experimental-modules ./bin/node-file-manager-esm.mjs -p 3000 -d /tmp/upload",
    "start-babel": "DEBUG=* node ./bin/node-file-manager-esm -p 3000 -d /tmp/upload",
    "start-esm": "DEBUG=* node ./bin/node-file-manager-esm -p 3000 -d /tmp/upload"
  },
  "bin": {
    "node-file-manager-esm": "./bin/node-file-manager-esm"
  },
  "dependencies": {
    "await-busboy": "^1.0.2",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "http-auth": "^3.2.3",
    "jszip": "^3.1.5",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "optimist": "^0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananaAcid/node-file-manager-esm.git"
  },
  "keywords": [
    "files",
    "manager",
    "koa",
    "angular",
    "web",
    "server",
    "modules",
    "mount",
    "esm",
    "es6"
  ],
  "author": {
    "name": "bananaacid",
    "email": "repo@bananaacid.de"
  },
  "contributers": [
    {
      "name": "efei",
      "email": "efeigm@gmail.com"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BananaAcid/node-file-manager-esm/issues"
  },
  "homepage": "https://github.com/BananaAcid/node-file-manager-esm#readme",
  "devDependencies": {
    "esm": "^3.0.84",
    "opn": "^5.4.0"
  }
}
