{
  "name": "node-file-manager-esm",
  "version": "2.0.2",
  "description": "File manager, based on Koa and Angular.js. Standalone, Koa-mountable. Import-able as ES6 and Babel module.",
  "main": "index.mjs",
  "engines": {
    "node": ">=10.5"
  },
  "scripts": {
    "start": "DEBUG=fm:* node --experimental-modules ./server.mjs",
    "start-debug": "DEBUG=* node --experimental-modules ./server.mjs -p 3000 -d /tmp/upload",
    "start-babel": "echo \"npm install the dev dependencies.\" && DEBUG=*,-babel node server.babel-entry.js -p 3000 -d /tmp/upload"
  },
  "bin": {
    "node-file-manager": "../node-file-manager/bin/node-file-manager-2.sh"
  },
  "dependencies": {
    "await-busboy": "^1.0.2",
    "debug": "^3.1.0",
    "fs-extra": "^6.0.1",
    "http-auth": "^3.2.3",
    "jszip": "^3.1.5",
    "koa": "^2.5.1",
    "koa-bodyparser": "^4.2.1",
    "koa-mount": "^3.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "optimist": "^0.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananaAcid/node-file-manager-2.git"
  },
  "keywords": [
    "files",
    "manager",
    "koa",
    "angular",
    "web",
    "server",
    "modules",
    "mount"
  ],
  "author": {
    "name": "efei",
    "email": "efeigm@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BananaAcid/node-file-manager-2/issues"
  },
  "homepage": "https://github.com/BananaAcid/node-file-manager-2#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-preset-stage-3": "^6.24.1"
  }
}
